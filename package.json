{
  "name": "@taxmanagementnz/n8n-nodes-azure-servicebus",
  "version": "1.1.0",
  "description": "Azure Service Bus nodes for n8n with full session support using official @azure/service-bus based on pokash community node",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/TaxManagementNewZealand/n8n-nodes-azure-servicebus",
  "author": {
    "name": "Lukasz Gabrysiak",
    "email": "l.gabrysiak@pokash.pl"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TaxManagementNewZealand/n8n-nodes-azure-servicebus.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes/**/*.ts credentials/**/*.ts package.json",
    "lintfix": "eslint nodes/**/*.ts credentials/**/*.ts package.json --fix",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AzureServiceBusApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AzureServiceBus/AzureServiceBus.node.js",
      "dist/nodes/AzureServiceBusTrigger/AzureServiceBusTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.27",
    "@types/node-fetch": "^2.6.4",
    "@types/ws": "^8.5.6",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "^0.135.0",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "@azure/service-bus": "^7.9.4",
    "node-fetch": "^2.7.0",
    "ws": "^8.14.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
