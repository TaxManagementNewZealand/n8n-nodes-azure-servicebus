{
  "name": "@taxmanagementnz/n8n-nodes-azure-servicebus",
  "version": "1.2.0",
  "description": "Azure Service Bus nodes for n8n with full session support using the official @azure/service-bus library",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://github.com/TaxManagementNewZealand/n8n-nodes-azure-servicebus",
  "author": {
    "name": "Tax Management New Zealand"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TaxManagementNewZealand/n8n-nodes-azure-servicebus.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes/**/*.ts credentials/**/*.ts",
    "lintfix": "eslint nodes/**/*.ts credentials/**/*.ts --fix",
    "test": "npx jest",
    "test:coverage": "npx jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AzureServiceBusApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/AzureServiceBus/AzureServiceBus.node.js",
      "dist/nodes/AzureServiceBusTrigger/AzureServiceBusTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/node-fetch": "^2.6.4",
    "@types/ws": "^8.5.6",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.6",
    "gulp": "^4.0.2",
    "jest": "^30.2.0",
    "n8n-workflow": "^1.120.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.4.6",
    "typescript": "~5.4.0"
  },
  "dependencies": {
    "@azure/service-bus": "^7.9.4",
    "node-fetch": "^2.7.0",
    "ws": "^8.19.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
